    "scripting": "just-build"
  },
  "just-build": {
    "default": [
      "just-build build push release"
    ],
    "build": [
      "ng build"
    ],
    "push": [
      "git add --all",
      "git commit -m 'chore: update src/packages/spotacard.app'"
    ],
    "release": [
      "standard-version",
      "git push --follow-tags origin master"
    ],
    "publish:package": [
      "VERSION=0.0.13",
      "cd src",
      "dotnet build LowLandTech.Api.csproj --configuration Release",
      "mkdir nuget",
      "dotnet pack LowLandTech.Api.csproj -p:PackageVersion=$VERSION --output nuget",
      "cd nuget",
      "dotnet nuget push *.nupkg -s https://nuget.lowlandtech.com -k oy2lhucx4hz4mg6qbgdqmcgs4naqsbnu7dktu6vnuxixzu",
      "cd ..",
      "rm -r nuget"
    ],
    "publish:image": [
      "VERSION=0.0.13",
      "docker login https://registry.lowlandtech.com -u wendellmva -p St@r2020",
      "docker build -t registry.lowlandtech.com/app:$VERSION -t registry.lowlandtech.com/app:latest .",
      "docker push registry.lowlandtech.com/app:latest",
      "docker push registry.lowlandtech.com/app:$VERSION"
    ]
  },
